{
    "name": "exegeseit/ezged-ws-client",
    "description": "This package aims to provide a client for ezGed Webservices",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Claude GLOMBARD",
            "email": "jc.glombard@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "psr/log": "^1.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "autoload": {
        "psr-4": {
            "ExeGeseIT\\EzGEDWsClient\\": "src/"
        }
    },
    "require-dev": {
        "monolog/monolog": "^1.24",
        "symfony/var-dumper": "^4.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
